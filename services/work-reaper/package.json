{
  "name": "harmony-work-reaper",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node app/server.ts",
    "start-dev": "strict-npm-engines && ts-node-dev --no-notify -r tsconfig-paths/register --watch app/views --respawn app/server",
    "start-dev-fast": "TS_NODE_TRANSPILE_ONLY=true ts-node-dev --no-notify -r tsconfig-paths/register --respawn --inspect=127.0.0.1:${DEBUG_PORT:-9204} app/server",
    "test": "eslint --ext .ts . && nyc mocha && better-npm-audit audit",
    "test-fast": "TS_NODE_TRANSPILE_ONLY=true mocha",
    "lint": "eslint --ext .ts .",
    "coverage": "nyc mocha",
    "prebuild": "./bin/prebuild",
    "prebuild-m1": "./bin/prebuild",
    "build": "tsc --project ./tsconfig.build.json && docker build -f Dockerfile.mac --tag harmonyservices/work-reaper:${VERSION:-latest} .",
    "build-m1": "tsc --project ./tsconfig.build.json && docker build -f Dockerfile.mac --platform linux/amd64 --tag harmonyservices/work-reaper:${VERSION:-latest} .",
    "push-image": "./bin/push-image ${VERSION:-latest}",
    "publish": "docker push harmonyservices/work-reaper:${VERSION:-latest}",
    "better-audit": "better-npm-audit audit"
  },
  "engines": {
    "node": "^22.5.1",
    "npm": ">=8"
  },
  "author": "NASA EOSDIS Harmony team",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.437.0",
    "@aws-sdk/client-sqs": "^3.437.0",
    "@aws-sdk/credential-provider-imds": "^3.374.0",
    "@aws-sdk/hash-node": "^3.374.0",
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.437.0",
    "@aws-sdk/url-parser": "^3.374.0",
    "@aws-sdk/util-format-url": "^3.433.0",
    "@harmony/util": "file:../../packages/util",
    "@types/node-fetch": "^2.5.12",
    "agentkeepalive": "^4.1.4",
    "axios": "^1.6.8",
    "axios-retry": "^3.8.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "knex": "^2.4.0",
    "knex-paginate": "^2.3.0",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^2.6.6",
    "pg": "^8.7.3",
    "pg-native": "^3.0.1",
    "tmp": "^0.2.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^8.3.2",
    "winston": "^3.10.0",
    "xstate": "^4.31.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.3",
    "@types/express": "^4.17.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^20.8.9",
    "@types/sinon": "^10.0.6",
    "@types/supertest": "^2.0.10",
    "axios-mock-adapter": "^1.20.0",
    "better-npm-audit": "^3.7.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.43.0",
    "mocha": "^9.1.3",
    "mockdate": "^3.0.2",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1",
    "superagent": "^8.0.6",
    "supertest": "^6.3.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.4.4"
  },
  "overrides": {
    "braces": "^3.0.3",
    "fast-xml-parser": "4.4.1"
  }
}
