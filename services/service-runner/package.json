{
  "name": "service-runner",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node app/server.ts",
    "start-dev": "strict-npm-engines && ts-node-dev --no-notify -r tsconfig-paths/register --watch app/views --respawn app/server",
    "start-dev-fast": "TS_NODE_TRANSPILE_ONLY=true ts-node-dev --no-notify -r tsconfig-paths/register --respawn --inspect=127.0.0.1:9231 app/server",
    "test": "eslint --ext .ts . && nyc mocha --timeout 5000 && better-npm-audit audit",
    "test-fast": "TS_NODE_TRANSPILE_ONLY=true mocha --timeout 5000",
    "lint": "eslint --ext .ts .",
    "coverage": "nyc mocha --timeout 5000",
    "prebuild": "./bin/prebuild",
    "prebuild-m1": "./bin/prebuild",
    "build": "tsc && docker build --tag harmonyservices/service-runner:${VERSION:-latest} .",
    "build-m1": "tsc && docker build --platform linux/amd64 --tag harmonyservices/service-runner:${VERSION:-latest} .",
    "push-image": "./bin/push-image ${VERSION:-latest}",
    "publish": "docker push harmonyservices/service-runner:${VERSION:-latest}",
    "better-audit": "better-npm-audit audit"
  },
  "engines": {
    "node": "^20.9.0",
    "npm": ">=8"
  },
  "author": "NASA EOSDIS Harmony team",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.342.0",
    "@aws-sdk/credential-provider-imds": "^3.342.0",
    "@aws-sdk/hash-node": "^3.342.0",
    "@aws-sdk/protocol-http": "^3.342.0",
    "@aws-sdk/s3-request-presigner": "^3.342.0",
    "@aws-sdk/url-parser": "^3.342.0",
    "@aws-sdk/util-format-url": "^3.342.0",
    "@harmony/util": "file:../../packages/util",
    "@kubernetes/client-node": "^0.17.1",
    "agentkeepalive": "^4.1.4",
    "aws-sdk": "^2.1029.0",
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "class-validator": "^0.14.0",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^3.2.10",
    "python-shell": "^3.0.1",
    "tmp": "^0.2.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.3",
    "@types/express": "^4.17.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^20.5.7",
    "@types/sinon": "^10.0.6",
    "@types/superagent": "^4.1.13",
    "@types/supertest": "^2.0.10",
    "axios-mock-adapter": "^1.20.0",
    "better-npm-audit": "^3.7.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copyfiles": "^2.3.0",
    "eslint": "^8.43.0",
    "mocha": "^9.1.3",
    "rimraf": "^5.0.1",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1",
    "superagent": "^8.0.6",
    "ts-node": "^10.4.0",
    "typescript": "^5.1.6"
  }
}