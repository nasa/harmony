{
  "name": "service-runner",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node app/server.ts",
    "start-dev": "strict-npm-engines && ts-node-dev --no-notify -r tsconfig-paths/register --watch app/views --respawn app/server",
    "start-dev-fast": "TS_NODE_TRANSPILE_ONLY=true ts-node-dev --no-notify -r tsconfig-paths/register --respawn --inspect=127.0.0.1:9231 app/server",
    "test": "eslint --ext .ts . && nyc mocha --timeout 5000 && better-npm-audit audit",
    "test-fast": "TS_NODE_TRANSPILE_ONLY=true mocha --timeout 5000",
    "lint": "eslint --ext .ts .",
    "coverage": "nyc mocha --timeout 5000",
    "prebuild": "./bin/prebuild",
    "prebuild-m1": "./bin/prebuild",
    "build": "tsc && docker build --tag harmonyservices/service-runner:${VERSION:-latest} .",
    "build-m1": "tsc && docker build --platform linux/amd64 --tag harmonyservices/service-runner:${VERSION:-latest} .",
    "build-multi": "./bin/prebuild && tsc && docker buildx build --platform=linux/amd64,linux/arm64 -t harmonyservices/service-runner:${VERSION:-latest} --push .",
    "push-image": "./bin/push-image ${VERSION:-latest}",
    "publish": "docker push harmonyservices/service-runner:${VERSION:-latest}",
    "better-audit": "better-npm-audit audit"
  },
  "engines": {
    "node": "^22.14.0",
    "npm": ">=8"
  },
  "author": "NASA EOSDIS Harmony team",
  "license": "Apache-2.0",
  "lockedDependenciesComments": {
    "chai": "Version 5 requires ESM which breaks running mocha. It's not clear what changes need to be made to fix it.",
    "chai-as-promised": "Version 8 requires ESM",
    "@types/chai": "Chai dependency is locked",
    "@types/chai-as-promised": "chai-as-promised dependency is locked",
    "eslint": "The eslint-config-airbnb-typescript project is no longer maintained and limits eslint < 9.",
    "@aws-sdk/client-s3": "Downgraded this library to a known working version due to performance issues."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.437.0",
    "@aws-sdk/credential-provider-imds": "^3.374.0",
    "@aws-sdk/hash-node": "^3.374.0",
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.437.0",
    "@aws-sdk/url-parser": "^3.374.0",
    "@aws-sdk/util-format-url": "^3.433.0",
    "@harmony/util": "file:../../packages/util",
    "@kubernetes/client-node": "^0.22.3",
    "agentkeepalive": "^4.1.4",
    "aws-sdk": "^2.1482.0",
    "axios": "^1.7.4",
    "axios-retry": "^4.5.0",
    "class-validator": "^0.14.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^3.2.10",
    "python-shell": "^5.0.0",
    "tmp": "^0.2.4",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/chai": ">=4.3.20 <5.0.0",
    "@types/chai-as-promised": ">=7.1.8 <8.0.0",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.1",
    "@types/sinon": "^17.0.3",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.12",
    "axios-mock-adapter": "^2.1.0",
    "better-npm-audit": "^3.7.3",
    "chai": ">=4.5.0 <5.0.0",
    "chai-as-promised": ">=7.1.2 <8.0.0",
    "copyfiles": "^2.3.0",
    "eslint": ">=8.57.1 <9.0.0",
    "mocha": "^11.0.1",
    "nyc": "^17.1.0",
    "rimraf": "^6.0.1",
    "sinon": "^12.0.1",
    "superagent": "^10.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^5.1.6",
    "ws": "^8.18.0"
  },
  "overrides": {
    "braces": "^3.0.3",
    "fast-xml-parser": "4.4.1",
    "jsonpath-plus": "^10.0.7",
    "cross-spawn": "^7.0.5",
    "form-data": "4.0.4"
  }
}
